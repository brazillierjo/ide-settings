{
  "format_on_save": "on",
  "soft_wrap": "editor_width",
  "autosave": "on_focus_change",
  "inlay_hints": {
    "enabled": true,
  },
  "cursor_shape": "underline",
  "ui_font_size": 15.0,
  "ui_font_family": "Hiragino Sans",
  "buffer_line_height": "comfortable",
  "buffer_font_weight": 400.0,
  "buffer_font_size": 15.0,
  "buffer_font_family": "Geist Mono",
  "buffer_font_features": {
    "calt": true,
    "liga": true,
  },
  "icon_theme": {
    "mode": "system",
    "light": "JetBrains New UI Icons (Light)",
    "dark": "JetBrains New UI Icons (Dark)",
  },
  "project_panel": {
    "hide_hidden": false,
    "hide_root": true,
    "scrollbar": {
      "show": "never",
    },
    "auto_fold_dirs": true,
    "indent_size": 25.0,
    "git_status": true,
    "folder_icons": true,
    "file_icons": true,
    "entry_spacing": "comfortable",
    "hide_gitignore": true,
    "default_width": 240.0,
    "dock": "left",
  },
  "theme": {
    "mode": "system",
    "light": "macOS Classic Light",
    "dark": "macOS Classic Dark",
  },
  "terminal": {
    "font_family": "JetBrains Mono NL",
    "line_height": "comfortable",
    "dock": "right",
    "default_width": 400,
  },
  "languages": {
    "TypeScript": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--write"],
        },
      },
    },
    "TSX": {
      "format_on_save": "on",
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--write"],
        },
      },
    },
  },
}
